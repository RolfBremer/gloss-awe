{
    "version": "0.2",
    "language": "en,de",
    "dictionaryDefinitions": [
        {
            "name": "CustomDE",
            "path": "VSSpell/de-DE_User.dic"
        },
        {
            "name": "CustomEN",
            "path": "VSSpell/en-US_User.dic"
        },
        {
            "name": "IgnoredWords",
            "path": "VSSpell/IgnoredWords.txt"
        }
    ],
    "dictionaries": [
        "CustomDE",
        "CustomEN",
        "IgnoredWords",
        "powershell",
        "csharp"
    ],
    "ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "**/bower_components/**",
        "**/vscode-extension/**",
        "**/.git/**",
        ".vscode",
        "typings",
        "**/*.sln.vsspell",
        "**/VSSpell/**",
        "**/Output/**",
        "**/dist/**",
        "**/bin/**",
        "**/obj/**",
        "*.binlog"
    ],
    "enableFiletypes": [
        "azure-pipelines",
        "csharp",
        "markdown",
        "powershell",
        "text",
        "typst",
        "xml",
        "yaml"
    ],
    "allowCompoundWords": true,
    "enabled": true
}
